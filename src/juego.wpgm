import wollok.game.*
import pepita.*
import visuales.*

program juego {
	plantilla.iniciar()
	fogata.iniciar()
	logo.iniciar()
	marley.iniciar()
	
	keyboard.space().onPressDo({partida.mostrarPregunta()})
	keyboard.num1().onPressDo({partida.responder("a")})
	keyboard.num2().onPressDo({partida.responder("b")})
	keyboard.enter().onPressDo({partida.siguientePregunta()})	

    game.start()
}